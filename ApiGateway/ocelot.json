{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "user-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/auth/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "Priority": 10
    },
    {
      "DownstreamPathTemplate": "/api/moderation/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "moderation-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/moderation/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "Priority": 10,
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      }
    },
    {
      "DownstreamPathTemplate": "/api/incidents/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "incident-service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/incidents/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "Priority": 10
    },
    {
      "DownstreamPathTemplate": "/api/incidents/upload-image",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "incident-service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/incidents/upload-image",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/images/incidents/{fileName}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "incident-service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/images/incidents/{fileName}",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}